  "homepage": "https://bitbucket.org/Michael_DiCarlo/postybirb-angular#readme",
  "scripts": {
    "ng": "ng",
    "compile": "ng build --vendor-chunk=false",
    "build": "ng build --vendor-chunk=false --aot",
